# Copy to .env and adjust values
FLASK_ENV=development
PORT=8000
UPLOAD_DIR=uploads
# DATABASE_URL format for psycopg v3 (note: Render provides postgresql:// which is auto-converted)
DATABASE_URL=postgresql+psycopg://postgres:postgres@localhost:5432/musication
# The external base URL when serving links (use http://localhost:8000 for local)
BASE_URL=http://localhost:8000
# Frontend URL for CORS in production (optional, leave empty for dev)
FRONTEND_URL=https://musicationapp.netlify.app
